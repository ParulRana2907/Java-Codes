//Experiment 6 
// q1...part2
interface A {
    void show();
}

interface B extends A {   // B extends A
    void display();
}

class MyClass implements B {
    public void show() {
        System.out.println("Parul Rana 24csu292");
        System.out.println("Show method from A");
    }
    public void display() {
        System.out.println("Display method from B");
    }
}

public class TestB {
    public static void main(String[] args) {
        MyClass obj = new MyClass();
        obj.show();
        obj.display();
    }
}

